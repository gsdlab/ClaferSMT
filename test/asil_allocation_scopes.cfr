abstract ComponentFailureEvent 24
  sil ->> integer
  [sil >= 0 && sil <= 4]

abstract CutSet 31
  fault -> Fault
  componentFailureEvent -> ComponentFailureEvent + 
    
abstract Fault 2
  sil ->> integer
  [sil >= 0 && sil <= 4]
    
[all cs : CutSet | sum(cs.componentFailureEvent.sil) >= cs.fault.sil]

