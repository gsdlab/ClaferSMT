abstract Dimension
	levels -> DimensionLevel *
	[all dl : this.levels | dl.belongsTo = this]

abstract DimensionLevel
	belongsTo -> Dimension
       
dim1 : Dimension
dim2 : Dimension

dimLevel1 : DimensionLevel
dimLevel2 : DimensionLevel

[some disj dl1;dl2 : DimensionLevel | dl1.belongsTo = dl2.belongsTo]